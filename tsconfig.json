{
	"extends": ["@sapphire/ts-config", "@sapphire/ts-config/bundler", "@docusaurus/tsconfig"],
	"compilerOptions": {
		"baseUrl": ".",
		"jsx": "react-jsx",
		"lib": ["ESNext", "DOM"],
		"noEmit": true,
		"noImplicitAny": false,
		"skipDefaultLibCheck": true
